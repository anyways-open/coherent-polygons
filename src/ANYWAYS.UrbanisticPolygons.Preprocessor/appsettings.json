{
    "Logging": {
        "LogLevel": {
            "Default": "Debug",
            "System": "Information",
            "Microsoft": "Information"
        }
    },
    "Serilog": {
        "Using": [
            "Serilog.Sinks.File",
            "Serilog.Sinks.Console"
        ],
        "MinimumLevel": "Verbose",
        "WriteTo": [
            {
                "Name": "File",
                "Args": {
                    "path": "logs/log-.txt",
                    "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog",
                    "rollingInterval": "Day"
                }
            },
            {
                "Name": "Console"
            }
        ]
    },
    "cache_folder": "/media/xivk/2T-SSD-EXT/temp",
    "tile_url": "https://data1.anyways.eu/tiles/full/20200628-150902/14/{x}/{y}.osm"
}